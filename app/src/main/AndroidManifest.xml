<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.findhome">

    <uses-permission android:name="android.permission.READ_PHONE_NUMBERS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/home_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/home_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.FindHome"
        tools:targetApi="31">
        <activity
            android:name=".pages.ShowAllImageActivity"
            android:exported="false" />
        <activity
            android:name=".pages.PendingItemDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".pages.AdminHomeActivity"
            android:exported="false" />
        <activity
            android:name=".pages.EditActivity"
            android:exported="false" />
        <activity
            android:name=".pages.AssetDetailActivity"
            android:exported="false" />
        <activity
            android:name=".pages.DetailsActivity"
            android:exported="false" />
        <activity
            android:name=".pages.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".pages.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".pages.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>